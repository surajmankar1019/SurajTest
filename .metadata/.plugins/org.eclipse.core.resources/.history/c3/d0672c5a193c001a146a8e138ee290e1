package com.cjc;

import java.util.Scanner;

public class ReverseOrder 
{
     public static void main(String[] args) 
     {
	    int arr[]=new int[6];
	    int i;
	    Scanner sc=new Scanner(System.in);
	    
	    for(i=6;i>=1;i--)
	    {
	    	System.out.println("Reverse order is:"+i);
	    }
	 }
}
