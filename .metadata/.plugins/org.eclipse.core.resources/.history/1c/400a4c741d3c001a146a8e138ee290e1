package com.cjc;

import java.util.Scanner;

public class ReverseOrder 
{
     public static void main(String[] args) 
     {
    	 int arr[]=new int[] {1,2,3,4,5};
    	 int i;
    	 System.out.println("Original Arrays:");
    	 for(i=1;i<=5;i++)
    	 {
    		 System.out.println(i);
    	 }
     }   
}
